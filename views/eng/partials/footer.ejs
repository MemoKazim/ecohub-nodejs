<footer id="footer">
    <div class="footer-container">
      <div class="footer-logo">
        <a href="/eng/index">
          <div class="footer-bottom-logo"></div>
        </a>
      </div>
      <div class="footer-top">
        <div class="col-1">
          <div class="footer-1st">
            <div class="content">
              <h3>About</h3>
              <p class="footer-par" style="margin-top: 1rem;">
                ECOHUB is a non-profit platform that aims to contribute to reducing the negative impact of plastic waste on the environment. 
              </p>
            </div>
          </div>
        </div>

        <div class="col-2">
          <h3>Projects</h3>
          <div class="footer-par">
            <ul class="footer-par-ul">
              <%projects.forEach((data)=>{ %>
                <li class="footer-par-ul-li">
                  <i class="fa-solid fa-circle-check"></i>
                  <a href="/eng/projects#<%=data._id%>"
                    ><%-data.title.en-%></a
                  >
                </li>
              <%});%>
            </ul>
          </div>
        </div>

        <div class="col-3">
          <h3>News</h3>
          <div class="footer-par">
            <ul class="footer-par-ul">
              <%news.forEach((data)=>{ %>
                <li class="footer-par-ul-li col-news">
                  <a href="/eng/news/<%=data._id%>">
                    <%-data.title.en-%>
                  </a>
                  <p>Published: <%-data.date.getDate()-%>.<%-data.date.getMonth()-%>.<%-data.date.getFullYear()-%></p>
                </li>
              <%});%>
            </ul>
          </div>
        </div>

        <div class="col-4">
          <h3>Contact</h3>
          <div class="footer-par">
            <ul class="footer-par-ul">
              <li class="footer-par-ul-li">
                <p>EKOHUB LLC</p>
              </li>
              <%contact.forEach((data)=>{%>
              <li>
                <p> <%-data.title.en-%>: <%-data.content.en-%></p>
              </li>
              <%});%>
            </ul>
          </div>
        </div>
      </div>
      <hr />
      <div class="footer-bottom" style="display: flex; justify-content: center;align-items: center; flex-direction: column; margin: 3rem 0;">
        
        <div class="icons">
          <%footer.forEach((logo) => {  %>
            <div class="footer-logos">
              <a href="<%=logo.url%>" target="_blank">
                <img src="/uploads/<%=logo.image%>" alt="<%=logo.name%>">
              </a>
            </div>
          <%});%>
          
        </div>
        <p style="margin-top: 1rem;">Â© All rights reserved. EKOHUB LLC <%-new Date().getFullYear()-%></p>
      </div>
    </div>
  </footer>
  <script src="/js/<%=script%>"></script>
</body>
</html>